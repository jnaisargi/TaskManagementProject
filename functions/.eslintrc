{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2023,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "indent": ["error", 4],
        "no-underscore-dangle":0,
        "import/extensions":0,
        "import/no-unresolved":0,
        "import/no-import-module-exports": 0,
    }
}
